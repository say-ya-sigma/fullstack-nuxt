{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prisma:init": "prisma init",
    "prisma:gen": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:format": "prisma format",
    "prisma:seed": "prisma db seed",
    "eslint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "eslint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "docker:testdb:up": "sudo docker compose up testdb -d --wait",
    "docker:testdb:down": "sudo docker compose down testdb",
    "prisma:testdb:push": "DATABASE_URL=mysql://root:root@localhost:13306/prisma && prisma db push",
    "prisma:testdb:seed": "DATABASE_URL=mysql://root:root@localhost:13306/prisma && prisma db seed",
    "vitest:server": "npm run docker:testdb:up && npm run prisma:testdb:push && npm run prisma:testdb:seed && vitest run && npm run docker:testdb:down",
    "postinstall": "nuxt prepare"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.5",
    "@nuxt/devtools": "latest",
    "@nuxt/ui-templates": "^1.3.1",
    "@prisma/client": "5.1.1",
    "@trpc/client": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "@types/node": "^18.17.1",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "element-plus": "^2.3.9",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "nuxt": "^3.6.5",
    "prisma": "^5.1.1",
    "trpc-nuxt": "^0.10.6",
    "tsx": "^3.12.7",
    "typescript": "^5.1.0",
    "unctx": "^2.3.1",
    "vitest": "^0.34.2",
    "vue": "^3.3.4",
    "zod": "^3.21.4"
  }
}
